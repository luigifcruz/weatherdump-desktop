    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/runtime": "^7.7.2",
    "babel-loader": "^8.0.6",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.2.0",
    "electron": "^7.1.1",
    "get-port": "^5.0.0",
