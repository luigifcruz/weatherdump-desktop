{
  "name": "weatherdump",
  "version": "0.0.0-alpha.3",
  "description": "WeatherDump GUI",
  "main": "electron.js",
  "scripts": {
    "release": "npm run webpack:prod && electron-builder -wml --publish=always",
    "release:pre": "npm run webpack:prod && electron-builder -wml --publish=always --config.publish.releaseType=prerelease",
    "build": "npm run webpack:prod && electron-builder -wml --publish=never",
    "build:mac": "npm run webpack:prod && electron-builder --mac --publish=never",
    "build:win": "npm run webpack:prod && electron-builder --win --publish=never",
    "build:linux": "npm run webpack:prod && electron-builder --linux --publish=never",
    "webpack:dev": "webpack --config webpack.config.dev.js && node server.dev.js",
    "electron:dev": "NODE_ENV=development electron electron.js",
    "webpack:prod": "webpack --config webpack.config.prod.js"
  },
  "author": {
    "name": "Open Satellite Project",
    "email": "luigifcruz@gmail.com"
  },
  "license": "MIT",
